                ...
            </datawriter_qos>
            -->
        </qos_profile>

        <qos_profile name="ReplierExampleProfile" base_name="BuiltinQosLib::Pattern.RPC">

            <!--
            <datareader_qos>
                ...
            </datareader_qos>
            <datawriter_qos>
                ...
            </datawriter_qos>
            -->
        </qos_profile>

    </qos_library>
</dds>
