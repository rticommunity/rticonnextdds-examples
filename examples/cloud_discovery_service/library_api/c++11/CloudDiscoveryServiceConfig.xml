<?xml version="1.0"?>
<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://community.rti.com/schema/7.0.0/rti_cloud_discovery_service.xsd">

    <configuration_variables>
        <element>
            <name>RTI_CDS_PORT</name>
            <value>7400</value>
        </element>
    </configuration_variables>

    <cloud_discovery_service name="LibraryAPIDemo">
        <transport>
            <element>
                <alias>builtin.udpv4</alias>
                <receive_port>$(RTI_CDS_PORT)</receive_port>
            </element>
        </transport>
    </cloud_discovery_service>
</dds>
