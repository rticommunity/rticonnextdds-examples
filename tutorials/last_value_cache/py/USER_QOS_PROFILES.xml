<dds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://community.rti.com/schema/current/rti_dds_profiles.xsd">
    <qos_library name="MyLibrary">
        <qos_profile name="MyProfile" is_default_qos="true">
            <base_name>
                <element>BuiltinQosLib::Generic.KeepLastReliable.TransientLocal</element>
            </base_name>
        </qos_profile>
        <qos_profile name="Last10Cache">
            <base_name>
                <element>BuiltinQosLib::Generic.KeepLastReliable.TransientLocal</element>
            </base_name>
            <!-- Up to this point we will only keep the last sample
            Lets update the history to keep the last 10 samples -->
            <datawriter_qos>
                <history>
                    <depth>10</depth>
                </history>
            </datawriter_qos>
            <datareader_qos>
                <history>
                    <depth>10</depth>
                </history>
            </datareader_qos>
        </qos_profile>
    </qos_library>
</dds>
